<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.twopibd.dactarbari.doctor.appointment">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.PatientActivityMap"></activity>
        <activity android:name=".Activity.AddWeeklyChamber2" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".Activity.MapsActivity3"
            android:label="@string/title_activity_maps3" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <activity
            android:name=".Activity.MapsActivity2"
            android:label="@string/title_activity_maps2" />
        <activity android:name=".Activity.MapsActivity" />
        <activity android:name=".Activity.SearchDrActivity" />
        <activity android:name=".Activity.ChamberDetailsActivityDr" />
        <activity android:name=".Activity.ChamberListActivityDr" />
        <activity android:name=".Activity.AddWeeklyChamberActivity" />
        <activity android:name=".Activity.AddDailyScheduleActivityDr" />
        <activity android:name=".Activity.AddScheduleDrActivityBigMenu" />
        <activity android:name=".Activity.PatientProfileUpdateActivity" />
        <activity android:name=".Activity.PatientProfileActivity" />
        <activity android:name=".Activity.FullScreenActivity" />
        <activity android:name=".Activity.PatientHomeActivity" />
        <activity android:name=".Activity.DrScheduleViewActivity" />
        <activity android:name=".Activity.BaseActivity" />
        <activity android:name=".Activity.ScheduleMenuActivity" />
        <activity android:name=".Activity.MySchedulesActivity" />
        <activity android:name=".Activity.TestActivity" />
        <activity android:name=".Activity.HomeActivityDoctor" />
        <activity
            android:name=".Activity.AddScheduleActivity"
            android:label="Add New Schedule"
            android:parentActivityName=".Activity.ScheduleMenuActivity" />
        <activity
            android:name=".Activity.PatientSignupActivity"
            android:label="Patient Signup"
            android:parentActivityName=".Activity.SignUpTypeActivity" />
        <activity
            android:name=".Activity.DrSignUpActivity"
            android:label="Doctor Signup"
            android:parentActivityName=".Activity.SignUpTypeActivity" />
        <activity
            android:name=".Activity.SignUpTypeActivity"
            android:label="Select Signup Type"
            android:parentActivityName=".Activity.LoginActivity" />
        <activity
            android:name=".Activity.LoginActivity"
            android:label="Login"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.SignUpActivity"
            android:label="SignUp"
            android:parentActivityName=".Activity.LoginActivity" />
        <activity
            android:name=".Activity.PhoneEnterActivityActivity"
            android:label="SignUp"
            android:parentActivityName=".Activity.LoginActivity" />
        <activity
            android:name=".Activity.CodeVerificationActivity"
            android:label="Sign Up"
            android:parentActivityName=".Activity.PhoneEnterActivityActivity" />
        <activity android:name=".Activity.HomeActivity" />
        <activity
            android:name=".Activity.FindDoctorActivity"
            android:label="Find Doctor"
            android:parentActivityName=".Activity.HomeActivity" />
        <activity android:name=".Activity.DoctorHomeActivity" />
        <activity
            android:name=".Activity.SearchResultActivity"
            android:label="Search result"
            android:parentActivityName=".Activity.FindDoctorActivity" />
        <activity
            android:name=".Activity.DrPersonalInfoActivity"
            android:parentActivityName=".Activity.DoctorHomeActivity" />
        <activity
            android:name=".Activity.ChamberDetailActivity"
            android:label="Book Appointment"
            android:parentActivityName=".Activity.SearchResultActivity" />
        <activity
            android:name=".Activity.SendBookingActivity"
            android:label="Booking Info"
            android:parentActivityName=".Activity.ChamberDetailActivity" />
        <activity
            android:name=".Activity.RecomendationDetailActivity"
            android:label="Test Recomendation"
            android:parentActivityName=".Activity.HomeActivity" />
        <activity
            android:name=".Activity.ConfirmedAppointmentDetailActivity"
            android:label="Appointment detail"
            android:parentActivityName=".Activity.HomeActivity" />
        <activity
            android:name=".Activity.SplashActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar.Fullscreen" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>